{
    "name": "chatsensei",
    "displayName": "ChatSensei",
    "description": "AI-powered creative assistant",
    "publisher": "Rongxin Liu",
    "repository": {
        "type": "git",
        "url": "https://github.com/rongxin-liu/ChatSensei.git"
    },
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.76.0"
    },
    "categories": [
        "Education",
        "Machine Learning",
        "Programming Languages"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "contributes": {
        "commands": [
            {
                "command": "chatsensei.ask",
                "title": "Ask ChatSensei"
            }
        ]
    },
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "16.x",
        "@types/vscode": "^1.76.0",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.56.0",
        "@vscode/test-electron": "^2.3.0",
        "eslint": "^8.36.0",
        "glob": "^8.1.0",
        "mocha": "^10.2.0",
        "typescript": "^4.9.5"
    },
    "dependencies": {
        "markdown-it": "^13.0.1",
        "markdown-it-highlightjs": "^4.0.1",
        "openai": "^3.2.1"
    }
}
